# Maguru Auto - Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# ============================================
# API Configuration
# ============================================
# Backend API URL (for any REST endpoints)
VITE_API_URL=http://localhost:8000

# ============================================
# Firebase Configuration
# ============================================
# Get these from Firebase Console > Project Settings
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# ============================================
# Cloudinary Configuration
# ============================================
# Get these from Cloudinary Dashboard
# Cloud Name: https://console.cloudinary.com/console
# Upload Preset: Create in Settings > Upload > Upload presets
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=maguruauto_preset
VITE_CLOUDINARY_API_KEY=your_cloudinary_api_key

# ============================================
# Application Settings
# ============================================
# Application name
VITE_APP_NAME=Maguru Auto

# Application URL (for links and redirects)
VITE_APP_URL=http://localhost:5173

# Environment: development, staging, production
VITE_APP_ENV=development

